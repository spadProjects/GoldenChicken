<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="keywords" content="#">
    <meta name="description" content="#">
    <title>جوجه طلایی | @ViewBag.Title</title>
    <!-- Fav and touch icons -->
    <link rel="icon" href="/Content/logo.png">
    <!-- Bootstrap -->
    <link href="/Content/client/css/bootstrap.min.css" rel="stylesheet">
    <!-- Fontawesome -->
    <link href="/Content/client/css/font-awesome.css" rel="stylesheet">
    <!-- Flaticons -->
    <link href="/Content/client/css/font/flaticon.css" rel="stylesheet">
    <!-- Swiper Slider -->
    <link href="/Content/client/css/swiper.min.css" rel="stylesheet">
    <!-- Range Slider -->
    <link href="/Content/client/css/ion.rangeSlider.min.css" rel="stylesheet">
    <!-- magnific popup -->
    <link href="/Content/client/css/magnific-popup.css" rel="stylesheet">
    <!-- Nice Select -->
    <link href="/Content/client/css/nice-select.css" rel="stylesheet">
    <!-- Custom Stylesheet -->
    <link href="/Content/client/css/style.css" rel="stylesheet">
    <link href="/Content/client/css/video-player.css" rel="stylesheet">
    <!-- Custom Responsive -->
    <link href="/Content/client/css/responsive.css" rel="stylesheet">
    <link href="/Content/client/css/style.min.css" rel="stylesheet">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700&amp;display=swap" rel="stylesheet">
    @Scripts.Render("~/bundles/jquery")
</head>

<body>
    @Html.Action("Navbar", "Home")
    @RenderBody()
    <!-- footer -->
    <div class="footer-top section-padding bg-black">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3 col-sm-4 col-6 mb-sm-20">
                    <div class="icon-box">
                        <span class="text-light-green"><i class="flaticon-credit-card-1"></i></span>
                        <span class="text-custom-white">پرداخت<br>ایمن</span>
                    </div>
                </div>

                <div class="col-md-3 col-sm-4 col-6 mb-sm-20">
                    <div class="icon-box">
                        <span class="text-light-green"><i class="flaticon-help"></i></span>
                        <span class="text-custom-white">پشتیبانی <br>24 ساعت 7 روز</span>
                    </div>
                </div>
                <div class="col-md-3 col-sm-4 col-6">
                    <div class="icon-box">
                        <span class="text-light-green"><i class="flaticon-truck"></i></span>
                        <span class="text-custom-white">ارسال <br>رایگان </span>
                    </div>
                </div>
                <div class="col-md-3 col-sm-4 col-6">
                    <div class="icon-box">
                        <span class="text-light-green"><i class="flaticon-guarantee"></i></span>
                        <span class="text-custom-white">تضمین<br>بهترین قیمت</span>
                    </div>
                </div>

            </div>
        </div>
    </div>
    @if (ViewBag.ContactUsPage == null)
    {
        @Html.Action("Footer", "Home", new { isContactUsPage = false })
    }
    else
    {
        @Html.Action("Footer", "Home", new { isContactUsPage = true })
    }
    <!-- Place all Scripts Here -->
    <!-- jQuery -->

    <script src="/Content/client/js/jquery.min.js"></script>
    <!-- Popper -->

    <script src="/Content/client/js/popper.min.js"></script>
    <!-- Bootstrap -->
    <script src="/Content/client/js/bootstrap.min.js"></script>
    <script src="/Content/client/js/videoPlayer.js"></script>
    <!-- Range Slider -->
    <script src="/Content/client/js/ion.rangeSlider.min.js"></script>
    <!-- Swiper Slider -->
    <script src="/Content/client/js/swiper.min.js"></script>
    <!-- Nice Select -->
    <script src="/Content/client/js/jquery.nice-select.min.js"></script>
    <!-- magnific popup -->
    <script src="/Content/client/js/jquery.magnific-popup.min.js"></script>
    <!-- Maps -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDnd9JwZvXty-1gHZihMoFhJtCXmHfeRQg"></script>
    <!-- sticky sidebar -->
    <script src="/Content/client/js/sticksy.js"></script>
    <script src="/Content/client/js/navbar.js"></script>

    <script src="/Content/client/js/munchbox.js"></script>
    <!-- Munch Box Js -->
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("scripts", required: false)
</body>


</html>